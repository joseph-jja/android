<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:orientation="horizontal"
android:layout_marginTop="5px"
android:layout_marginBottom="5px"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<ImageView
android:id="@+id/feeds_image"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
/>

<LinearLayout
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_marginLeft="5px"
android:orientation="vertical"
>
<TextView
android:textAppearance="@style/FeedsText"
android:id="@+id/feeds_row_name"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:layout_gravity="left"
>
</TextView>

<LinearLayout
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:orientation="horizontal"
>
<TextView
android:id="@+id/feeds_row_count_lud"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_gravity="left"
android:textAppearance="@style/DefaultText"
>
</TextView>
<TextView
android:textAppearance="@style/DefaultText"
android:id="@+id/feeds_row_count"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_marginLeft="5px"
>
</TextView>
<TextView
android:id="@+id/feeds_row_id"
android:layout_width="fill_parent"
android:layout_height="0px"
>
</TextView>
</LinearLayout>

</LinearLayout>

</LinearLayout>
